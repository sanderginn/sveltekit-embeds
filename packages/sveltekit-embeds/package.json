{
	"name": "sveltekit-embeds",
	"version": "0.0.1",
	"description": "Svelte 5 embed components for media and social platforms.",
	"author": "Sander Ginn",
	"license": "MIT",
	"homepage": "https://github.com/sanderginn/sveltekit-embeds#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sanderginn/sveltekit-embeds.git",
		"directory": "packages/sveltekit-embeds"
	},
	"bugs": {
		"url": "https://github.com/sanderginn/sveltekit-embeds/issues"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:utils": "vitest run src/lib/utils/index.test.ts",
		"pack:dry-run": "pnpm run prepack && npm pack --dry-run",
		"publish:npm": "pnpm run prepack && pnpm publish --access public"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@vitest/browser-playwright": "^4.0.18",
		"playwright": "^1.57.0",
		"publint": "^0.3.17",
		"svelte": "^5.49.2",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.1"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"embed",
		"embeds",
		"youtube",
		"spotify",
		"tiktok",
		"twitch",
		"social"
	]
}
